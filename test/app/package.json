{
  "name": "sample-web-app",
  "version": "0.1.0",
  "description": "Sample Web Application",
  "main": "dist/server.js",
  "repository": "",
  "private": true,
  "author": "",
  "scripts": {
    "start": "node dist/server/server",
    "build": "babel server --out-dir dist/server --copy-files --source-maps",
    "watch": "babel server --out-dir dist/server --watch --copy-files --source-maps",
    "themost": "themost"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/register": "^7.0.0",
    "@types/angularjs": "^1.5.14-alpha",
    "@types/express": "^4.16.0",
    "@types/morgan": "^1.7.35",
    "@types/passport": "^0.4.7",
    "@types/supertest": "^2.0.6",
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "eslint": "^4.19.1",
    "mocha": "^5.2.0",
    "supertest": "^3.3.0"
  },
  "peerDependencies": {
    "@themost/common": "^2.1.6",
    "@themost/data": "^2.1.20",
    "@themost/sqlite": "^2.1.5",
    "@themost/web": "^2.1.16"
  },
  "dependencies": {
    "@themost/sqlite": "^2.1.5",
    "@types/chai": "^4.1.7",
    "bootstrap": "^4.1.0",
    "jquery": "^3.3.1",
    "source-map-support": "^0.5.10"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": false,
      "node": true,
      "es6": true,
      "jquery": false
    },
    "globals": {
      "describe": false,
      "it": false,
      "angular": false,
      "jQuery": false,
      "before": false,
      "after": false
    },
    "extends": "eslint:recommended",
    "rules": {
      "no-console": "warn",
      "no-invalid-this": "warn",
      "no-undef": "error",
      "no-unused-vars": "warn",
      "no-var": [
        "error"
      ],
      "strict": [
        2,
        "never"
      ]
    }
  }
}
